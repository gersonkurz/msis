<Wix xmlns="http://wixtoolset.org/schemas/v4/wxs" xmlns:util="http://wixtoolset.org/schemas/v4/wxs/util" xmlns:bal="http://wixtoolset.org/schemas/v4/wxs/bal">
  <!--
    WiX Bundle (Bootstrapper) Template - Silent Mode
    Generated by msis-3.x

    Install folder uses MSI defaults. Override via command line:
      setup.exe /install /passive INSTALLDIR="C:\Custom\Path"
  -->

  <Bundle Name="{{PRODUCT_NAME}}" Version="{{PRODUCT_VERSION}}" Manufacturer="{{MANUFACTURER}}" UpgradeCode="{{UPGRADE_CODE}}">

    <!-- Default install folder for bundle -->
    <Variable Name="InstallFolder" Type="string" Value="" bal:Overridable="yes"/>
    <SetVariable Id="SetInstallFolder" Variable="InstallFolder"
        Value="[ProgramFiles6432Folder]{{#if INSTALL_FOLDER}}{{INSTALL_FOLDER}}{{else}}{{#if INSTALLDIR}}{{INSTALLDIR}}{{else}}{{PRODUCT_NAME}}{{/if}}{{/if}}"/>

    <BootstrapperApplication>
      <bal:WixStandardBootstrapperApplication LicenseUrl="" Theme="none"/>
    </BootstrapperApplication>

    <Chain>
{{{CHAIN}}}
    </Chain>

  </Bundle>
</Wix>
